<html lang="en-gb">
<header><%- include('_head') -%>
        <nav><%- include('_header') -%></nav>
</header>

<body>
    <h1>Users</h1>
    <ol>
        <% results.forEach(function(user) {%>
            <li>
                <a href="/users/<%=user.id%>">
                    <%= user.name %>
                </a>
            </li>
            <% }); %>
    </ol>
    <%if (next) { %>
        <a href="<%= next %>">Next</a>
        <% } %>
            <%if (previous) { %>
                <a href="<%= previous %>">Previous</a>
                <% } %>
                    <p><a href="/users/create">Create A New User</p>
</body>
<footer><%- include('_footer') -%></footer>

</html>